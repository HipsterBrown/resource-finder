{
  "name": "Package Search",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "An extension for searching NPM & Bower for front-end packages for use with CodePen and other apps like it.",
  "homepage_url": "http://hipsterbrown.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Package Search",
    "default_popup": "src/browser_action/index.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
